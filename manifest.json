{
  "name": "HackTheU - Lazy eye",
  "version": "1.0",
  "description": "HackTheU",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "pages/popup.html"
  },
  "options_page": "pages/options.html",
  "permissions": ["storage", "declarativeContent", "activeTab"],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  }
}
